<Window x:Name="FNAF_Save_File_Editor_April_Fools" x:Class="FNAFSaveEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FNAFSaveEditor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="yes">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="49*"/>
            <ColumnDefinition Width="17*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="705*"/>
        </Grid.ColumnDefinitions>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="FNAF 1 Save File Editor" VerticalAlignment="Top" Margin="10,21,0,0" Height="103" Width="202" Grid.ColumnSpan="4"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="14" Margin="305,72,0,0" VerticalAlignment="Top" Width="0" RenderTransformOrigin="0.5,0.5" Grid.Column="3">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-177.504"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button Content="Unlock All&#xD;&#xA;" Click="Button_Click" HorizontalAlignment="Left" Height="30" Margin="10,42,0,0" VerticalAlignment="Top" Width="127" Grid.ColumnSpan="4"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="0.29,0.806" Grid.ColumnSpan="4" Height="35" Width="127"><Run Text="FNAF 2 Save File Editor"/><LineBreak/><Run/></TextBlock>
        <Button Content="Unlock All" Click="Button2_Click" HorizontalAlignment="Left" Margin="10,109,0,0" VerticalAlignment="Top" Grid.ColumnSpan="4" Height="28" Width="127"/>
        <TextBlock HorizontalAlignment="Left" Height="43" Margin="10,155,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147" Grid.ColumnSpan="4"><Run Text="FNAF 3 Save File Editor"/><LineBreak/><Run/></TextBlock>
        <Button Content="Unlock All" Click="Button3_Click" HorizontalAlignment="Left" Margin="10,183,0,0" VerticalAlignment="Top" Grid.ColumnSpan="4" Height="34" Width="127"/>


    </Grid>
</Window>
